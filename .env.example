# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_PROJECT_ID=

# AI Configuration (Using Google Gemini)
GOOGLE_AI_API_KEY=your_actual_google_ai_api_key_here
NEXT_PUBLIC_GOOGLE_AI_API_KEY=your_actual_google_ai_api_key_here

# Encryption Keys
ENCRYPTION_KEY=
JWT_SECRET=

# Database URLs
DATABASE_URL=

# Security
NEXTAUTH_SECRET=
NEXTAUTH_URL=http://localhost:3000

# PWA Configuration
NEXT_PUBLIC_APP_NAME="CreditRepairPro"
NEXT_PUBLIC_APP_DESCRIPTION="AI-powered credit repair and monitoring platform"

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_PWA=true
NEXT_PUBLIC_ENABLE_OFFLINE_MODE=true

# API Rate Limiting
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=100

# Stripe Configuration (for payments)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=

# File Upload
MAX_FILE_SIZE=10485760
ALLOWED_FILE_TYPES=pdf,jpg,jpeg,png,doc,docx

# Environment
NODE_ENV=development
NEXT_PUBLIC_ENV=development