{
  "name": "creditai",
  "version": "0.1.0",
  "private": true,
  "description": "AI-powered credit repair and monitoring SaaS platform",
  "keywords": [
    "credit repair",
    "ai",
    "finance",
    "nextjs",
    "typescript"
  ],
  "author": "CreditAI Team",
  "license": "MIT",
  "homepage": "https://creditai.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/creditai/app"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:unit": "jest --testPathPattern='src/lib.*/__tests__/.*\\.test\\.(js|ts|tsx)' --watchAll=false",
    "test:integration": "jest --testPathPattern='src/__tests__/integration/.*\\.test\\.(js|ts|tsx)' --watchAll=false",
    "test:performance": "jest --config=jest.config.performance.js --watchAll=false",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance",
    "analyze": "node scripts/analyze-bundle.js",
    "analyze:size": "size-limit",
    "perf:check": "node scripts/check-performance.js",
    "clean": "rm -rf .next out dist",
    "setup-env": "node scripts/setup-env.js",
    "setup-google-ai": "node scripts/setup-google-ai.js",
    "setup-storage": "node scripts/setup-storage.js",
    "setup-google-cloud": "node scripts/setup-google-cloud-complete.js",
    "setup-google-cloud-env": "node scripts/setup-google-cloud-env.js",
    "setup-google-cloud-env-complete": "node scripts/setup-complete-google-cloud-env.js",
    "configure-google-cloud-credentials": "node scripts/configure-google-cloud-credentials.js",
    "validate-google-cloud-env": "node scripts/validate-google-cloud-env.js",
    "test-google-cloud-env": "node scripts/test-google-cloud-env.js",
    "setup-google-cloud-cli": "node scripts/setup-google-cloud-cli.js",
    "setup-service-account": "node scripts/setup-service-account.js",
    "test-service-account": "node scripts/test-service-account.js",
    "setup-document-ai-processors": "node scripts/setup-document-ai-processors.js",
    "test-document-ai-processors": "node scripts/test-document-ai-processors.js",
    "validate-document-ai-setup": "node scripts/validate-document-ai-setup.js",
    "setup-supabase": "node scripts/setup-supabase.js",
    "configure-auth": "node scripts/configure-auth.js",
    "supabase:start": "supabase start",
    "supabase:stop": "supabase stop",
    "supabase:reset": "supabase db reset",
    "supabase:push": "supabase db push",
    "test-google-cloud": "node scripts/test-google-cloud-config.js",
    "test-gc-config": "node scripts/quick-google-cloud-test.js",
    "creditai": "node scripts/creditai-cli.js",
    "setup": "node scripts/master-infrastructure-deploy.js",
    "setup:quick": "node scripts/master-infrastructure-deploy.js --skip-confirmation --mode development",
    "validate": "node scripts/comprehensive-validation-suite.js",
    "validate:quick": "node scripts/check-setup.js",
    "diagnose": "node scripts/infrastructure-diagnostics.js",
    "infrastructure:deploy": "node scripts/master-infrastructure-deploy.js",
    "infrastructure:validate": "node scripts/comprehensive-validation-suite.js",
    "infrastructure:diagnose": "node scripts/infrastructure-diagnostics.js",
    "setup-monitoring": "node scripts/setup-monitoring.js",
    "start-monitoring": "bash start-monitoring.sh",
    "stop-monitoring": "docker-compose -f docker-compose.monitoring.yml down",
    "monitoring-logs": "docker-compose -f docker-compose.monitoring.yml logs -f",
    "monitoring-status": "docker-compose -f docker-compose.monitoring.yml ps"
  },
  "dependencies": {
